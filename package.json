  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/josemarluedke/ember-cli-daterangepicker.git"
  },
  "bugs": {
    "url": "https://github.com/josemarluedke/ember-cli-daterangepicker/issues"
